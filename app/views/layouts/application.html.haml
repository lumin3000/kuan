!!! 5
%html
  %head
    %title
      = content_for?(:title) ? yield(:title) : "-"
    = stylesheet_link_tag :all
    - js 'rails', 'mootools-core', 'application'
    - @js.uniq.reverse!.each do |f|
      = javascript_include_tag f
    = csrf_meta_tag
    = yield :head
  %body
    #container
      = yield
