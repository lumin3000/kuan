if($('blog_<%= @blog.id %>')){
  if(<%= @follow %>){
    $('tar_follow_<%= @blog.id %>').getElement('.btn_unfollow').show()
    $('tar_follow_<%= @blog.id %>').getElement('.btn_follow').hide()
  }else{
    $('tar_follow_<%= @blog.id %>').getElement('.btn_unfollow').hide()
    $('tar_follow_<%= @blog.id %>').getElement('.btn_follow').show()
  }
}
